#include <stdio.h>
#include <stdlib.h>

float A_ini[4][4]={{1,2,3,0},{4,5,6,0},{7,8,9,0},{0,0,0,1}};
float A_T[4][4]={0};
int main(int argc, char *argv[]) 
{
  	printf("\nOriginal Matrix:\n ");
	printf("\t%f\t %f\t %f\t %f\t\n",A_ini[0][0],A_ini[0][1],A_ini[0][2],A_ini[0][3]);
	printf("\t%f\t %f\t %f\t %f\t\n",A_ini[1][0],A_ini[1][1],A_ini[1][2],A_ini[1][3]);
	printf("\t%f\t %f\t %f\t %f\t\n",A_ini[2][0],A_ini[2][1],A_ini[2][2],A_ini[2][3]);
	printf("\t%f\t %f\t %f\t %f\t\n\n",A_ini[3][0],A_ini[3][1],A_ini[3][2],A_ini[3][3]);
	
	MatTranspose(A_ini,  A_T);
	printf("Input Matrix Transposed:\n ");
	printf("\b\t%f\t %f\t %f\t %f\t\n",A_T[0][0],A_T[0][1],A_T[0][2],A_T[0][3]);
	printf("\t%f\t %f\t %f\t %f\t\n",A_T[1][0],A_T[1][1],A_T[1][2],A_T[1][3]);
	printf("\t%f\t %f\t %f\t %f\t\n",A_T[2][0],A_T[2][1],A_T[2][2],A_T[2][3]);
	printf("\t%f\t %f\t %f\t %f\t\n\n",A_T[3][0],A_T[3][1],A_T[3][2],A_T[3][3]);
	
	
	MatInverse(A_ini, A_T);
	printf("Input Matrix Inverted:\n ");
	printf("\b\t%f\t %f\t %f\t %f\t\n",A_T[0][0],A_T[0][1],A_T[0][2],A_T[0][3]);
	printf("\t%f\t %f\t %f\t %f\t\n",A_T[1][0],A_T[1][1],A_T[1][2],A_T[1][3]);
	printf("\t%f\t %f\t %f\t %f\t\n",A_T[2][0],A_T[2][1],A_T[2][2],A_T[2][3]);
	printf("\t%f\t %f\t %f\t %f\t\n\n",A_T[3][0],A_T[3][1],A_T[3][2],A_T[3][3]);
}
